mixin slide-nav()
	+b.slide-nav&attributes(attributes)
		+e.inner
			+e.left-column
				nav
					+e.A.link.link(href='works.html') Мои работы
					+e.A.link.link(href='about.html') Об мне
					+e.A.link.link(href='blog.html') Блог
			+e.right-column
				address
					+e.UL.list
						+e.LI.item
						
					
			
