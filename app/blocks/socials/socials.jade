mixin socials(obj)
	- var icon = obj.icons || []
	+b(class='socials_#{obj.modifier}')&attributes(attributes)
		+e.UL(class='list_#{obj.modifier}')
			+e.LI(class='item_#{obj.modifier}')
				+e.A.link._vk.link(href='https://vk.com/id268045389') vkontact
					+icon(obj.name1, 31, 17)
			+e.LI(class='item_#{obj.modifier}')
				+e.A.link._github.link(href='https://github.com/stakeout') github
					+icon(obj.name2, 26, 22)
			+e.LI(class='item_#{obj.modifier}')
				+e.A.link._linkedin.link(href='') linkedin
					+icon(obj.name3, 24, 22)
